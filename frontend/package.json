{
  "name": "redstone-oracle-monitoring-frontend",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "parcel": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-json-view": "^1.21.3",
    "react-query": "^3.38.0",
    "react-router-dom": "6.3.0",
    "react-select": "^5.3.2",
    "recharts": "^2.1.10"
  },
  "devDependencies": {
    "@types/node": "^17.0.36",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.12",
    "process": "^0.11.10",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "populate-env": "grep -a -m 1 -h -r 'BACKEND_URL' ../.env | head -1 > .env",
    "dev": "rm -rf .parcel-cache && yarn populate-env && parcel index.html",
    "build": "yarn populate-env && parcel build index.html",
    "docker:build": "parcel build index.html"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
